[Unit]
Description=HIM Education AP Web Server
After=network.target him-dnsmasq.service him-ap.service him-firewall.service
Wants=him-dnsmasq.service him-ap.service him-firewall.service

[Service]
Type=simple
ExecStart=/usr/bin/python3 /home/him/walled_garden/server.py
WorkingDirectory=/home/him/walled_garden
Restart=always
User=root

[Install]
WantedBy=multi-user.target